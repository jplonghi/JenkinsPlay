pipeline {
	agent any
	environment {
		imageName = "docker.slc.crengland.com/cre/myservice:dev-1.0.${env.BUILD_NUMBER}"
    }
	stages{
		
		
		stage('Clone repository') {
			steps{
                checkout scm
			}
		}

		stage('Build Code'){
			steps{
				bat "echo ${env.BRANCH_NAME}"
			}
		  
		}
		
        stage('Deploy DEV'){
			steps{
			    bat "echo ${imageName}"
			}
		}
    }

}