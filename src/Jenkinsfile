pipeline {
	agent any
	environment {
		imageName = "docker.slc.crengland.com/cre/myservice:dev-1.0.${env.BUILD_NUMBER}"
    }
	stages{
		
		
		stage('Clone repository') {
			steps{
                checkout scm
			}
		}

		stage('Master'){
			when{
				branch 'master'
			}
			steps{
				bat "echo Build Master Branch"
			}
		  
		}
		
        stage('Feature'){
			when{
				branch 'feature'
			}
			steps{
				bat "echo Build feature branch"
			}
		}
    }

}